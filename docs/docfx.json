{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif",
          "**/*.svg"
        ]
      }
    ],
    "xref": [
      "xrefmaps/xrefmap-dotnet.zip",
      "xrefmaps/xrefmap-skiasharp.zip"
    ],
    "markdownEngineName": "markdig",
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "_appTitle": "SkiaSharp",
      "_enableSearch": true,
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/logo.ico",
      "_gitContribute": {
        "repo": "https://github.com/mono/SkiaSharp"
      },
      "_gitUrlPattern": "github"
    },
    "dest": "../_site",
    "template": [
      "default",
      "modern"
    ]
  }
}