<?xml version="1.0" encoding="utf-8" ?>
<Project>

  <PropertyGroup>
    <WasmBuildNative Condition="'$(WasmBuildNative)' == ''">true</WasmBuildNative>
  </PropertyGroup>

  <ItemGroup>
    <NativeFileReference Include="$(SkiaSharpStaticLibraryPath)\2.0.23\*.a" />
  </ItemGroup>

  <ItemGroup>
    <_TempStaticWebAsset Include="$(MSBuildThisFileDirectory)..\staticwebassets\*.js" />
    <StaticWebAsset Include="@(_TempStaticWebAsset->'$([System.IO.Path]::GetFullPath(%(Identity)))')">
      <SourceType>Package</SourceType>
      <SourceId>SkiaSharp.Views.Blazor</SourceId>
      <ContentRoot>$([System.IO.Path]::GetFullPath('$(MSBuildThisFileDirectory)..\staticwebassets\'))</ContentRoot>
      <BasePath>_content/SkiaSharp.Views.Blazor</BasePath>
      <RelativePath>%(Filename)%(Extension)</RelativePath>
      <AssetKind>All</AssetKind>
      <AssetMode>All</AssetMode>
      <AssetRole>Primary</AssetRole>
      <RelatedAsset></RelatedAsset>
      <AssetTraitName></AssetTraitName>
      <AssetTraitValue></AssetTraitValue>
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
      <OriginalItemSpec>$([System.IO.Path]::GetFullPath('%(Identity)'))</OriginalItemSpec>
    </StaticWebAsset>
  </ItemGroup>

</Project>
